class Main{
    public static void main(String[] args){
 int num[] = {40,50,60,70,80};
    int i, value = 70, pos=-1;

    for(i=0;i<=num.length;i++){
        if(value==num[i])
        {
            pos=i+1;
            break;
        }
    }
        if(pos==-1)
        {
            System.out.println("Number not found");
        }
        else {
            System.out.println("The value is found at "+pos);
        }
    }
}
    
